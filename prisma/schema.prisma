// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model FoodDetail {
  id           Int     @id @default(autoincrement())
  name         String  @map("food_name")
  price       Int
  calorie      Int?
  carbohydrate Int?
  fat          Int?
  protein      Int?
  vitamin      String?

  @@map("food_details")
}
